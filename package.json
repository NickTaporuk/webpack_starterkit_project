{
  "name": "webpack_starterkit_project",
  "version": "1.0.0",
  "description": "starterkit for webpack project",
  "main": "main.js",
  "scripts": {
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:dev": "webpack-dev-server --inline --content-base public/ --history-api-fallback",
    "start:prod": "npm run build && node server.bundle.js",
    "build:client": "webpack",
    "build:server": "webpack --config webpack.server.config.js",
    "build": "npm run build:client && npm run build:server"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/NickTaporuk/webpack_starterkit_project.git"
  },
  "author": "Nicholay Kuropatkin",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/NickTaporuk/webpack_starterkit_project/issues"
  },
  "homepage": "https://github.com/NickTaporuk/webpack_starterkit_project#readme",
  "devDependencies": {
    "autoprefixer": "6.4.1",
    "babel-core": "^6.14.0",
    "babel-eslint": "^6.1.2",
    "babel-loader": "^6.2.5",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-stage-0": "^6.5.0",
    "css-loader": "0.24.0",
    "eslint": "^3.5.0",
    "eslint-loader": "^1.5.0",
    "eslint-plugin-react": "^6.2.2",
    "express": "^4.14.0",
    "jshint-loader": "^0.8.3",
    "jsx-loader": "^0.13.2",
    "npm-install-webpack-plugin": "4.0.4",
    "postcss-loader": "0.13.0",
    "precss": "1.4.0",
    "react": "^15.3.1",
    "react-dom": "^15.3.1",
    "react-hot-loader": "^1.3.0",
    "redux-devtools": "^3.3.1",
    "style-loader": "0.13.1"
  },
  "dependencies": {
    "babel-loader": "^6.2.5",
    "babel-polyfill": "^6.13.0",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-react": "^6.11.1",
    "css-loader": "^0.24.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "jshint": "^2.9.3",
    "node-sass": "^3.8.0",
    "react-redux": "4.4.5",
    "react-router": "^2.7.0",
    "redux": "3.6.0",
    "redux-logger": "2.6.1",
    "redux-thunk": "2.1.0",
    "sass-loader": "^4.0.1",
    "style-loader": "^0.13.1"
  }
}
